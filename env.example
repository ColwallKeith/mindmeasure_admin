# Mind Measure Environment Configuration

# Application Environment
NODE_ENV=development
VITE_APP_ENV=development

# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# AWS Cognito Configuration
VITE_AWS_COGNITO_USER_POOL_ID=your_cognito_user_pool_id
VITE_AWS_COGNITO_CLIENT_ID=your_cognito_client_id
VITE_AWS_COGNITO_IDENTITY_POOL_ID=your_cognito_identity_pool_id

# Database Configuration
VITE_DATABASE_URL=postgresql://username:password@your-aurora-endpoint:5432/mindmeasure
VITE_BACKEND_PROVIDER=aurora-serverless

# API Configuration
VITE_API_URL=https://api.mindmeasure.co.uk
VITE_APP_URL=https://app.mindmeasure.co.uk

# AWS S3 Configuration
VITE_AWS_S3_BUCKET=mindmeasure-assets
VITE_AWS_S3_REGION=us-east-1

# AWS Lambda Configuration
VITE_AWS_LAMBDA_ENDPOINT=https://your-lambda-endpoint.amazonaws.com

# ElevenLabs Configuration (Optional)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key
VITE_ELEVENLABS_AGENT_ID=your_agent_id

# Feature Flags
VITE_ENABLE_VOICE_ASSESSMENT=true
VITE_ENABLE_COST_MONITORING=true
VITE_ENABLE_SECURITY_DASHBOARD=true

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_LOGGING=true


      "scripts": {
            "dev": "vite",
            "build": "vite build",
            "vercel-build": "vite build",
            "start": "vite preview",
            "typecheck": "tsc --noEmit",
            "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
            "guard:aws": "echo 'AWS configuration guard - checking for exposed secrets'",
            "guard:cross": "echo 'Cross-import guard placeholder - add specific checks as needed'",
            "guard:all": "npm run guard:aws && npm run guard:cross",
            "test:e2e": "playwright test",
            "test:e2e:ui": "playwright test --ui",
            "demo:preflight": "node scripts/demo-preflight.js",
            "demo:tag": "node -e \"const d=new Date();const p=s=>String(s).padStart(2,'0');const tag=`demo-${d.getFullYear()}${p(d.getMonth()+1)}${p(d.getDate())}-${p(d.getHours())}${p(d.getMinutes())}`;console.log(tag);\" | xargs -I {} sh -c 'git tag {} && git push origin --tags && echo Tagged {}'",
            "demo:build": "npm run build",
            "deploy:check": "npm run typecheck && npm run lint && npm run build",
            "deploy:production": "vercel --prod",
            "deploy:preview": "vercel",
            "build:mobile": "npm run build && npx cap sync",
            "build:ios": "npm run build:mobile && npx cap open ios",
            "build:android": "npm run build:mobile && npx cap open android",
            "build:all": "npm run deploy:check && npm run build:mobile",
            "api": "node server/api.js",
            "dev:full": "concurrently \"npm run api\" \"npm run dev\"",
            "install:server": "npm install express cors pg concurrently"
      }
